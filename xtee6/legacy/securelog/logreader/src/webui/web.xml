<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>legacy-logreader</display-name>

	<filter>
		<filter-name>wicket.logreader</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>ee.ria.xroad_legacy.logreader.LogReaderUIApplication</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>wicket.logreader</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>ExtractSvt</servlet-name>
		<servlet-class>ee.ria.xroad_legacy.logreader.ExtractSvt</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExtractSvt</servlet-name>
		<url-pattern>/asic</url-pattern>
	</servlet-mapping>

</web-app>
