<jxb:bindings 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
    version="2.1">

    <jxb:bindings schemaLocation="identifiers.xsd">
        <jxb:bindings node="//xs:complexType[@name='SdsbClientIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.ClientId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbServiceIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.ServiceId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbSecurityCategoryIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.SecurityCategoryId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbCentralServiceIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.CentralServiceId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbSecurityServerIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.SecurityServerId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbGlobalGroupIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.GlobalGroupId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbLocalGroupIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.LocalGroupId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='SdsbIdentifierType']">
            <jxb:class ref="ee.cyber.sdsb.common.identifier.SdsbId"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>
