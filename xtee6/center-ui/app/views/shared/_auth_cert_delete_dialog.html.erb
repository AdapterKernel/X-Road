<%= dialog "auth_cert_delete_dialog", t("securityservers.auth_cert.delete.title") do %>
  <section class="container">
    <legend><%= t 'securityservers.auth_cert.owner.legend' %></legend>
    <table class="details">
      <tr>
        <td><%= t 'securityservers.owner_name' %></td>
        <td><p id="delete_auth_cert_owner_name" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.owner_class' %></td>
        <td><p id="delete_auth_cert_owner_class" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.owner_code' %></td>
        <td><p id="delete_auth_cert_owner_code" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.server_code' %></td>
        <td><p id="delete_auth_cert_servercode" class="input"></p></td>
      </tr>
    </table>
  </section>

  <section class="container">
    <legend><%= t 'securityservers.auth_cert.cert.legend' %></legend>
    <table class="details">
      <tr>
        <td><%= t 'securityservers.auth_cert.ca' %></td>
        <td><p id="delete_auth_cert_ca" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.auth_cert.serial_number' %></td>
        <td><p id="delete_auth_cert_serial_number" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.auth_cert.subject' %></td>
        <td><p id="delete_auth_cert_subject" class="input"></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.auth_cert.expires' %></td>
        <td><p id="delete_auth_cert_expires" class="input"></p></td>
      </tr>
    </table>
  </section>
<% end %>
