<%= dialog "owned_server_add_dialog", t("management_requests.details.auth_cert_reg.new.title") do %>
  <section class="container">
    <legend><%= t 'securityservers.info.legend' %></legend>
    <table class="details">
      <tr>
        <td><%= t 'securityservers.owner_name' %></td>
        <td><p id="owned_server_owner_name" class="input" ></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.owner_class' %></td>
        <td><p id="owned_server_owner_class" class="input" ></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.owner_code' %></td>
        <td><p id="owned_server_owner_code" class="input" ></p></td>
      </tr>
      <tr>
        <td><%= t 'securityservers.server_code' %></td>
        <td><input id="owned_server_add_servercode" type="text"/></td>
      </tr>
    </table>
  </section>

  <section class="container">
    <legend><%= t 'securityservers.auth_cert.cert.legend' %></legend>
    <table>
      <tr>
        <td>
          <%= form_tag "members/import_auth_cert",
              :id => "auth_cert_upload", :multipart => true,
              :target => "upload_target" do %>
            <%= file_field_tag "auth_cert_file" %>
          <% end %>
        </td>
        <iframe id="upload_target" name="upload_target"></iframe>
        <td>
          <% if can?(:add_security_server_auth_cert_reg_request) %>
            <button id="owned_server_cert_upload">
              <%= t 'securityservers.auth_cert.upload' %>
            </button>
          <% end %>
        </td>
      </tr>
      <tr class="auth_cert_details">
        <td><%= t 'securityservers.auth_cert.ca' %></td>
        <td><p id="owned_server_authcert_csp" class="input" ></p></td>
      </tr>
      <tr class="auth_cert_details">
        <td><%= t 'securityservers.auth_cert.serial_number' %></td>
        <td><p id="owned_server_authcert_serial_number" class="input" ></p></td>
      </tr>
      <tr class="auth_cert_details">
        <td><%= t 'securityservers.auth_cert.subject' %></td>
        <td><p id="owned_server_authcert_subject" class="input" ></p></td>
      </tr>
      <tr class="auth_cert_details">
        <td><%= t 'securityservers.auth_cert.expires' %></td>
        <td><p id="owned_server_authcert_expires" class="input" ></p></td>
      </tr>
    </table>
    <p id="owned_server_temp_cert_id" class="ui-helper-hidden"></p>
  </section>
<% end %>
