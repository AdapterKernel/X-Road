<% content_for :head do %>
  <%= javascript_include_tag "distributed_files" %>
<% end %>

<% content_for :content do %>
  <% if can?(:view_distributed_files) %>

    <div class="fill" id="distributed_files-holder">
      <fieldset>
        <table id="distributed_files">
          <thead>
            <tr>
              <th><%= t 'distributed_files.filetype' %></th>
              <th><%= t 'distributed_files.info' %></th>
              <th>&nbsp;</th>
            </tr>
          </thead>
        </table>

        <div class="dialog_dynamic">
          <%= render :partial => "identifier_mapping_upload_dialog" %>
          <%= render :partial => "application/console_output_dialog" %>
        </div>
      </fieldset>
    </div>
  <% end %>
<% end %>
