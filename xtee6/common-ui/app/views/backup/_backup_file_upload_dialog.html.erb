<%= dialog "backup_file_upload_dialog", t("backup.upload.title") do %>

  <% if can?(:backup_configuration) %>
    <%= form_tag "#{controller.controller_name}/upload_new",
        :id => "upload_new_backup_file", :multipart => true,
        :target => "new_backup_file_upload_target" do %>
      <%= upload_button("new_backup_file_upload") %>
    <% end %>
  <% end %>

  <iframe id="new_backup_file_upload_target" name="new_backup_file_upload_target"></iframe>
<% end %>
