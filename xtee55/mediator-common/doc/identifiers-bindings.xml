<jxb:bindings 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
    version="2.1">

    <jxb:bindings schemaLocation="identifiers.xsd">
        <jxb:bindings node="//xs:complexType[@name='XroadClientIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.ClientId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadServiceIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.ServiceId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadSecurityCategoryIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.SecurityCategoryId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadCentralServiceIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.CentralServiceId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadSecurityServerIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.SecurityServerId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadGlobalGroupIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.GlobalGroupId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadLocalGroupIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.LocalGroupId"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='XroadIdentifierType']">
            <jxb:class ref="ee.ria.xroad.common.identifier.XroadId"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>
