<table class="details">
  <tr>
    <td><%= t '.friendly_name' %>:</td>
    <td>
      <form>
        <input name="token_id" type="hidden" value="<%= @token.id %>"/>
        <input name="friendly_name" type="text" style="float: right;"
               value="<%= @token.friendlyName || @token.id %>"
               <%= "disabled" unless token_saved_to_configuration?(@token) && can?(:edit_keytable_friendly_names) %>/>
      </form>
    </td>
  </tr>
</table>

<section class="container">
  <h2><span><%= t '.token_info' %></span></h2>
  <pre>
    <%= t '.token_id' %>: <%= @token.id %>
<% unless @token.tokenInfo.isEmpty %>
<% @token.tokenInfo.each do |key, value| %>    <%= key %>: <%= value %>
<% end %>
<% end %>
  </pre>
</section>
